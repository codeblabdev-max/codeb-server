{
  "name": "codeb-cli",
  "version": "1.0.0",
  "description": "CodeB CLI - Local Project Management Tool",
  "main": "bin/codeb.js",
  "bin": {
    "codeb": "./bin/codeb.js"
  },
  "scripts": {
    "start": "node bin/codeb.js",
    "dev": "nodemon bin/codeb.js",
    "build": "pkg . -t node18-macos-x64,node18-linux-x64,node18-win-x64",
    "install-local": "npm link",
    "uninstall-local": "npm unlink"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "conf": "^11.0.2",
    "figlet": "^1.7.0",
    "cli-progress": "^3.12.0",
    "node-ssh": "^13.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "pkg": "^5.8.1"
  },
  "keywords": ["cli", "codeb", "project-management", "podman"],
  "author": "CodeB Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}