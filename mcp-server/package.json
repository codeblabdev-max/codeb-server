{
  "name": "codeb-mcp",
  "version": "7.0.64",
  "description": "CodeB v7.0 - Unified HTTP API Server with Blue-Green Deployment + Claude Code 2.1 Integration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "codeb-mcp": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeb-dev-run/codeb-server.git",
    "directory": "mcp-server"
  },
  "homepage": "https://github.com/codeb-dev-run/codeb-server#readme",
  "bugs": {
    "url": "https://github.com/codeb-dev-run/codeb-server/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "predev": "../scripts/check-version.sh || true",
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "db:migrate": "tsx src/scripts/migrate.ts"
  },
  "keywords": [
    "codeb",
    "deployment",
    "blue-green",
    "mcp",
    "ci-cd",
    "claude-code"
  ],
  "author": "CodeB Team",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "helmet": "^8.0.0",
    "pg": "^8.13.0",
    "prom-client": "^15.1.0",
    "ssh2": "^1.16.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.0",
    "@types/pg": "^8.11.0",
    "@types/ssh2": "^1.15.1",
    "eslint": "^9.16.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.2.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist"
  ]
}
