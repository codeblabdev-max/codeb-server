{
  "name": "@codeb/analytics",
  "version": "6.0.5",
  "description": "CodeB Analytics SDK - Web Vitals and custom metrics collection",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./next": {
      "import": "./dist/next.js",
      "types": "./dist/next.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/react.tsx src/next.tsx --format esm --dts --clean",
    "dev": "tsup src/index.ts src/react.tsx src/next.tsx --format esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "web-vitals": "^4.2.4"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "next": ">=14.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "codeb",
    "analytics",
    "web-vitals",
    "performance",
    "monitoring"
  ],
  "author": "CodeB Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anthropics/codeb-analytics.git"
  }
}
